		int a[] = {3,4,5,2,1};
		for(int i=0;i<a.length;i++)
		{
			for(int j=0;j<n-i-1;j++)
			{
				if(a[i]>a[j])
				{
					int temp = a[i];
					a[i]=a[j];
					a[j]=temp;
				}
			}
			System.out.println(a[i]);
		}
		

	}

}
