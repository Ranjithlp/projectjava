import java.util.List;
public class BubbleSort {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a[] = {3,4,5,2,1};
		for(int i=0;i<a.length-1;i++)
		{
			//for(int j=0;j<a.length-i-1;j++)
			//{
				if(a[i]>a[i+1])
				{
					int temp = a[i];
					a[i]=a[i+1];
					a[i+1]=temp;
				}
			//}
			System.out.println(a[i]);
		}
		

	}

}
